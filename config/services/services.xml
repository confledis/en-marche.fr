        </service>
        <service id="App\EventListener\ProcurationReliabilitySubscriber"/>
        <service id="App\EventListener\ControllerListener">
            <tag name="kernel.event_listener" event="kernel.controller" method="onKernelController" priority="8" />
        </service>
