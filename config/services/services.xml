            </call>
        </service>
        <service id="App\Validator\UniqueTurnkeyProjectPinnedValidator" />
        <service id="App\Validator\UniqueVotePlaceAssessorValidator" />
        <service id="App\Validator\ValidAdherentCoReferentValidator" />

        <!-- Biography -->
