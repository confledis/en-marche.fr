        <service id="App\Doctrine\VisibleStatusesExtension">
            <tag name="api_platform.doctrine.orm.query_extension.collection" priority="9" />
        </service>
        <service id="App\Doctrine\MyIdeaContributionExtension">
            <tag name="api_platform.doctrine.orm.query_extension.collection" priority="9" />
